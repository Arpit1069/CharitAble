<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homefragdonor"
    android:orientation="horizontal"
    tools:context=".dish_res">

    <androidx.cardview.widget.CardView
        android:layout_width="190dp"
        android:layout_height="250dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="120dp"
        app:cardBackgroundColor="#D3D1D1"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardUseCompatPadding="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/button_food"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginTop="35dp"
                android:src="@drawable/restaurant"
                tools:ignore="SpeakableTextPresentCheck" />
            <!--                tools:ignore="SpeakableTextPresentCheck" />-->


        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="190dp"
        android:layout_height="250dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="120dp"
        app:cardBackgroundColor="#D3D1D1"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardUseCompatPadding="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/button_books"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginTop="35dp"
                android:src="@drawable/image_book"
                tools:ignore="SpeakableTextPresentCheck" />
<!--                tools:ignore="SpeakableTextPresentCheck" />-->


        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="190dp"
        android:layout_height="250dp"
        android:layout_marginStart="113dp"
        android:layout_marginTop="380dp"
        app:cardBackgroundColor="#D3D1D1"
        app:cardCornerRadius="15dp"
        app:cardElevation="10dp"
        app:cardUseCompatPadding="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/button_clothes"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:layout_marginTop="35dp"
                android:src="@drawable/clothes"
                tools:ignore="SpeakableTextPresentCheck" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>


</RelativeLayout>

